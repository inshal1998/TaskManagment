{
    "version": 2,
    "builds": [
      {
        "src": "app.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ],
    "env": {
      "MONGO_URI": "mongodb+srv://<username>:<password>@cluster0.mongodb.net/task?retryWrites=true&w=majority",
      "PORT": "3000"
    }
  }
  